{
    "private": true,
    "scripts": {
        "build": "turbo run build",
        "dev": "turbo run dev",
        "lint": "turbo run lint",
        "desktop": "yarn workspace desktop",
        "web": "yarn workspace web",
        "api": "yarn workspace api",
        "format": "prettier --write \"**/*.{ts,tsx,md}\"",
        "desktop:dev": "yarn turbo --filter api --filter desktop dev"
    },
    "devDependencies": {
        "eslint": "^8.48.0",
        "prettier": "^3.0.3",
        "tsconfig": "*",
        "turbo": "latest"
    },
    "name": "kurumi",
    "packageManager": "yarn@1.22.10",
    "workspaces": [
        "apps/*",
        "packages/*"
    ],
    "dependencies": {
        "prisma": "^5.4.2"
    }
}
